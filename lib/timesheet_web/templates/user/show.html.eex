<h1>Show User</h1>

<ul>

  <li>
    <strong>User:</strong>
    <%= @user.user_id %>
  </li>

  <li>
    <strong>User name:</strong>
    <%= @user.user_name %>
  </li>

  <li>
    <strong>Password:</strong>
    <%= @user.password %>
